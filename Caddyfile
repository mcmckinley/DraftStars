# Route traffic to the frontend service
brawlmind.com {	
	reverse_proxy frontend:3000
}

# This always produces an error message. Ignore it. It works.
brawlmind.com/api/* {
	reverse_proxy backend:8889
}

plausible.brawlmind.com {
	reverse_proxy plausible:8000
}
